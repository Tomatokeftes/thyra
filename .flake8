[flake8]
max-line-length = 79
extend-ignore = E203, W503
exclude =
    .git,
    __pycache__,
    build,
    dist,
    .eggs,
    *.egg-info,
    .tox,
    .venv,
    env,
    venv,
    ENV,
    env.bak,
    venv.bak,
    # Ignore auto-generated ontology files
    msiconvert/metadata/ontology/_*.py,
